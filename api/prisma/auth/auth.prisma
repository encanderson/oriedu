generator client {
  provider = "prisma-client-js"
  output   = "../../node_modules/@internal/prisma/auth"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_AUTH")
}

model User {
  id         String   @id @default(uuid())
  user_id    String   @unique
  name       String
  active     Boolean
  app        String
  code       Int?
  created_at DateTime
  email      String   @unique
  password   String?
  picture    String?
  updated_at DateTime
  consents   Json?

  @@map("users")
}
