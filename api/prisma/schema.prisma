generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRESQL_URI")
}



model User {
  id         String   @id @default(uuid())
  userId     String   @unique
  active     Boolean
  app        String
  code       Int?
  createdAt  DateTime
  email      String   @unique
  password   String
  profession String
  updatedAt  DateTime
  consents   Json?

  @@map("users")
}


